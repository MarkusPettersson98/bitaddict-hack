{
  "name": "CatAddict",
  "version": "1.0",
  "description": "Everyone needs a cat once in a while",
  "icons": {
    "16": "cute_cat.png",
    "32": "cute_cat.png",
    "48": "cute_cat.png",
    "128": "cute_cat.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "cute_cat.png",
      "32": "cute_cat.png",
      "48": "cute_cat.png",
      "128": "cute_cat.png"
    }
  },
  "permissions": ["tabs", "contextMenus"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "manifest_version": 2
}

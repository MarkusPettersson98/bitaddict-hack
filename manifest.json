{
  "name": "CatAddict",
  "version": "1.0",
  "description": "Everyone needs a cat once in a while",
  "icons": {
      "16": "cute_cat.jpeg",
      "32": "cute_cat.jpeg",
      "48": "cute_cat.jpeg",
      "128": "cute_cat.jpeg"
    },
  "browser_action": {
          "default_icon": {
            "16": "cute_cat.png",
            "32": "cute_cat.png",
            "48": "cute_cat.png",
            "128": "cute_cat.png"
          }
  },
  "permissions": [
      "tabs", "contextMenus"
    ],
    "background": {
      "scripts": ["background.js"]
    },

  "manifest_version": 2
}
